{
  "name": "arsnova",
  "private": true,
  "version": "1.0.0",
  "description": "A desktop app for PDF OCR processing",
  "author": "Do365 Technologies Private Limited",
  "main": "electron/main.js",
  "homepage": "./",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"wait-on http://localhost:5173 && npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "cross-env NODE_ENV=development electron .",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "electron-builder -c electron.config.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "dist": "npm run build && electron-builder -c electron.config.js --publish=never",
    "pack": "electron-builder -c electron.config.js --dir"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@fontsource/nunito": "^5.2.7",
    "@napi-rs/canvas": "^0.1.80",
    "@pdf-lib/fontkit": "^1.1.1",
    "@popperjs/core": "^2.11.6",
    "@reduxjs/toolkit": "^2.2.7",
    "@supabase/supabase-js": "^2.57.4",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.4.0",
    "animate.css": "^4.1.1",
    "bcryptjs": "^2.4.3",
    "canvas": "^3.2.0",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "fontkit": "^2.0.4",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "multer": "^1.4.5-lts.1",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdf-to-png-converter": "^3.10.0",
    "pdf2pic": "^3.2.0",
    "pdfjs-dist": "^5.4.149",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-popper": "^2.3.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.4.2",
    "sharp": "^0.34.4",
    "sqlite3": "^5.1.7",
    "tesseract.js": "^5.1.1",
    "wait-on": "^8.0.1",
    "web-vitals": "^4.2.3",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.17",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "electron-is-dev": "^3.0.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
