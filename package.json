{
  "name": "Do365 Tech",
  "private": true,
  "version": "1.0.0",
  "description": "A desktop app for PDF OCR processing",
  "author": "Do365 Technologies Private Limited",
  "main": "electron/main.js",
  "homepage": "./",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"wait-on http://localhost:5173 && npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "cross-env NODE_ENV=development electron .",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "electron-builder -c electron.config.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "dist": "npm run build && electron-builder -c electron.config.js --publish=never",
    "pack": "electron-builder -c electron.config.js --dir"
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.80",
    "@pdf-lib/fontkit": "^1.1.1",
    "@supabase/supabase-js": "^2.57.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "bcryptjs": "^2.4.3",
    "canvas": "^3.2.0",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "fontkit": "^2.0.4",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "multer": "^1.4.5-lts.1",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdf-to-png-converter": "^3.10.0",
    "pdf2pic": "^3.2.0",
    "pdfjs-dist": "^5.4.149",
    "quill": "^2.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-quill": "^2.0.0",
    "sharp": "^0.34.4",
    "sqlite3": "^5.1.7",
    "tesseract.js": "^5.1.1",
    "wait-on": "^8.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "electron-is-dev": "^3.0.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}